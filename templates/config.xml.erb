<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <logRotator>
    <daysToKeep>
      <%= has_variable?("days_to_keep") ? days_to_keep : '-1' %>
    </daysToKeep>
    <numToKeep>
      <%= has_variable?("num_to_keep") ? num_to_keep : '-1' %>
    </numToKeep>
    <artifactDaysToKeep>
      <%= has_variable?("artifact_days_to_keep") ? artifact_days_to_keep : '-1' %>
    </artifactDaysToKeep>
    <artifactNumToKeep>
      <%= has_variable?("artifact_num_to_keep") ? artifact_num_to_keep : '-1' %>
    </artifactNumToKeep>
  </logRotator>
  <keepDependencies>
    <%= has_variable?("keep_dependencies") ? keep_dependencies : 'false' %>
  </keepDependencies>
  <properties>
    <% if has_variable?("github_url") %>
    <%= scope.function_template("_github.xml.erb") %>
    <% end %>
  </properties>
  <% if has_variable?("scm") && scm == "git" %>
  <%= scope.function_template("_git_scm.xml.erb") %>
  <% end %>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <% if has_variable?("github_push_trigger") && github_push_trigger %>
    <com.cloudbees.jenkins.GitHubPushTrigger>
      <spec></spec>
    </com.cloudbees.jenkins.GitHubPushTrigger>
    <% end %>
    <% if has_variable?("build_schedule") %>
    <hudson.triggers.SCMTrigger>
      <spec><%= build_schedule %></spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command><%= build_script %></command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <% if has_variable?("build_wrappers") %>
    <% build_wrappers.each do |bw| %>
    <%= "<#{bw}/>" %>
    <% end %>
    <% end %>
  </buildWrappers>
</project>
